{
  "development": {
    "username": "${DB_USER}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_POSTGRES}",
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "dialect": "postgres",
    "logging": false
  },
  "production": {
    "use_env_variable": "DB_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "logging": false
  }
}
